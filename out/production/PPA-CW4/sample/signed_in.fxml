<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>

<HBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <Circle fill="WHITE" radius="25.0" stroke="BLACK" strokeType="INSIDE" />
      <Button mnemonicParsing="false" onAction="#signOutAction" text="Sign out" />
      <Button mnemonicParsing="false" style="-fx-background-image: url(&quot;nopfp.png&quot;);; -fx-background-size: 50px, 50px; -fx-background-repeat: no-repeat; -fx-background-position: center; -fx-border-radius: 30;" />
   </children>
</HBox>
