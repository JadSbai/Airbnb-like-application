<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>

<HBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <VBox>
         <children>
            <Circle fx:id="profileCircle" fill="WHITE" onMouseClicked="#profileClicked" radius="25.0" stroke="BLACK" strokeType="INSIDE" />
         </children>
      </VBox>
      <Button mnemonicParsing="false" onAction="#signOutAction" text="Sign out" />
   </children>
</HBox>
