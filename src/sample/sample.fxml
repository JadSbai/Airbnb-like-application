<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>

<BorderPane xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <SplitPane dividerPositions="0.5" orientation="VERTICAL">
        <items>
            <StackPane prefHeight="150.0" prefWidth="200.0">
               <children>
                  <Label text="Welcome Text" />
               </children>
            </StackPane>
            <BorderPane prefHeight="200.0" prefWidth="200.0">
               <left>
                  <CheckBox mnemonicParsing="false" text="CheckBox" BorderPane.alignment="CENTER" />
               </left>
            </BorderPane>
        </items>
      </SplitPane>
   </center>
   <top>
      <StackPane prefHeight="25.0" prefWidth="593.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Welcome To AirBnb" />
         </children>
      </StackPane>
   </top>
   <bottom>
      <BorderPane BorderPane.alignment="CENTER">
         <left>
            <Button mnemonicParsing="false" text="&lt;" BorderPane.alignment="CENTER" />
         </left>
         <right>
            <Button mnemonicParsing="false" text="&gt;" BorderPane.alignment="CENTER" />
         </right>
      </BorderPane>
   </bottom>
</BorderPane>
