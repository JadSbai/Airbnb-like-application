<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" styleClass="label" stylesheets="@StatisticsPanelStyle.css" xmlns:xsi="http://javafx.com/fxml" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.StatisticsController">
    <children>
        <BorderPane prefHeight="201.0" prefWidth="640.0">
            <center>
                <GridPane BorderPane.alignment="CENTER">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <BorderPane prefHeight="200.0" prefWidth="200.0">
                            <left>
                                <Button fx:id="backButtonStatistics1" mnemonicParsing="false" onAction="#backButtonStatistics1Pressed" prefHeight="100.0" text="&lt;" BorderPane.alignment="CENTER" />
                            </left>
                            <right>
                                <Button fx:id="forwardButtonStatistics1" mnemonicParsing="false" onAction="#backButtonStatistics1Pressed" prefHeight="100.0" text="&gt;" BorderPane.alignment="CENTER" />
                            </right>
                            <center>
                                <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                                    <children>
                                        <Label fx:id="statistic1" alignment="TOP_CENTER" text="Average number of reviews per property" textAlignment="CENTER" wrapText="true" />
                                    </children>
                                </VBox>
                            </center>
                        </BorderPane>
                        <BorderPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1">
                            <left>
                                <Button fx:id="backButtonStatistics2" mnemonicParsing="false" onAction="#backButtonStatistics2Pressed" prefHeight="100.0" text="&lt;" BorderPane.alignment="CENTER" />
                            </left>
                            <right>
                                <Button fx:id="forwardButtonStatistics2" mnemonicParsing="false" onAction="#backButtonStatistics2Pressed" prefHeight="100.0" text="&gt;" BorderPane.alignment="CENTER" />
                            </right>
                            <center>
                                <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                                    <children>
                                        <Label fx:id="statistic2" alignment="TOP_CENTER" text="Total number of available properties" textAlignment="CENTER" wrapText="true" />
                                    </children>
                                </VBox>
                            </center>
                        </BorderPane>
                        <BorderPane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1">
                            <left>
                                <Button fx:id="backButtonStatistics3" mnemonicParsing="false" onAction="#backButtonStatistics3Pressed" prefHeight="100.0" text="&lt;" BorderPane.alignment="CENTER" />
                            </left>
                            <right>
                                <Button fx:id="forwardButtonStatistics3" mnemonicParsing="false" onAction="#backButtonStatistics3Pressed" prefHeight="100.0" text="&gt;" BorderPane.alignment="CENTER" />
                            </right>
                            <center>
                                <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                                    <children>
                                        <Label fx:id="statistic3" text="The number of entire home and apartments" textAlignment="CENTER" wrapText="true" />
                                    </children>
                                </VBox>
                            </center>
                        </BorderPane>
                        <BorderPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                            <left>
                                <Button fx:id="backButtonStatistics4" mnemonicParsing="false" onAction="#backButtonStatistics4Pressed" prefHeight="100.0" text="&lt;" BorderPane.alignment="CENTER" />
                            </left>
                            <right>
                                <Button fx:id="forwardButtonStatistics4" mnemonicParsing="false" onAction="#backButtonStatistics4Pressed" prefHeight="100.0" text="&gt;" BorderPane.alignment="CENTER" />
                            </right>
                            <center>
                                <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                                    <children>
                                        <Label fx:id="statistic4" alignment="TOP_CENTER" contentDisplay="CENTER" text="The most expensive borough." textAlignment="CENTER" wrapText="true" />
                                    </children>
                                </VBox>
                            </center>
                        </BorderPane>
                    </children>
                </GridPane>
            </center>
        </BorderPane>
    </children>
</VBox>
